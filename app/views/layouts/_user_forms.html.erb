<!-- This partial should be in the users directory -->
<!-- It is a user-related partial, not a layout template -->

<h5>Become a member of Viz!</h5>
<div>
  <%= f.label :first_name, "First Name"%>
  <%= f.text_field :first_name%>
</div>
<div>
  <%= f.label :last_name, "Last Name"%>
  <%= f.text_field :last_name %>
</div>
<div>
  <%= f.label :city, "City"%>
  <%= f.text_field :city %>
</div>
<div>
  <%= f.label :email, "Email"%>
  <%= f.email_field :email %>
</div>
<div>
  <%= f.label :image, "Profile Picture" %>
  <%= f.url_field :image %>
</div>
<div>
  <%= f.label :password, "Password"%>
  <%= f.password_field :password %>
</div>
