<%= form_for @user do |f| %>
      <div>
        <%= f.label :first_name%>
        <%= f.text_field :first_name %>
      </div>
      <div>
        <%= f.label :last_name%>
        <%= f.text_field :last_name %>
      </div>
      <div>
        <%= f.label :city%>
        <%= f.text_field :current_city %>
      </div>
      <div>
        <%= f.label :image%>
        <%= f.url_field :image %>
      </div>
      <div>
        <%= f.label :email%>
        <%= f.email_field :email %>
      </div>
      <%= f.submit "Save Changes", class: "btn" %>
      <% end %>
      <!-- indentation!! -->
